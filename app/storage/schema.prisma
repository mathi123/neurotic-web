datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL") // uses connection pooling
    directUrl = env("DATABASE_URL_UNPOOLED") // uses a direct connection
}

generator client {
    provider        = "prisma-client-js"
    output          = "client"
    previewFeatures = ["queryCompiler", "driverAdapters"]
}

model Car {
    id   String @id @default(uuid())
    name String
}
